<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://xml.calculadora.com/restarRequest"
    targetNamespace="http://xml.calculadora.com/restarRequest"
    elementFormDefault="qualified">
    
    <xsd:element name="restarRequest">
        <xsd:complexType>
            <xsd:sequence> 
                
                <xsd:element name="minuendo" minOccurs="1" maxOccurs="1">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:decimal">
                            <xsd:annotation>
                                <xsd:documentation>Minuendo de la operacion</xsd:documentation>
                            </xsd:annotation>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>

                <xsd:element name="sustraendo" minOccurs="1" maxOccurs="1">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:decimal">
                            <xsd:annotation>
                                <xsd:documentation>Sustraendo de la operacion</xsd:documentation>
                            </xsd:annotation>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>